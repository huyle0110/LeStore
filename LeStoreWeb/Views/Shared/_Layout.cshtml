@{
    ViewBag.Title = "Home Page";
}
<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="76x76" href="~/Content/Layout/assets/img/apple-icon.png">
    <link rel="icon" type="image/png" sizes="96x96" href="~/Content/Layout/assets/img/favicon.png">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

    <title>Le'Store</title>

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />

    <!-- Bootstrap core CSS     -->
    <link href="~/Content/Layout/assets/css/bootstrap.min.css" rel="stylesheet" />

    <!--  Paper Dashboard core CSS    -->
    <link href="~/Content/Layout/assets/css/paper-dashboard.css" rel="stylesheet" />
    <link href="~/Content/Styles.css" rel="stylesheet" />

    <!--  CSS for Demo Purpose, don't include it in your project     -->
    <link href="~/Content/Layout/assets/css/demo.css" rel="stylesheet" />

    <!--  Fonts and icons     -->
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
    <link href='https://fonts.googleapis.com/css?family=Muli:400,300' rel='stylesheet' type='text/css'>
    <link href="~/Content/Layout/assets/css/themify-icons.css" rel="stylesheet">
</head>

<body>
    <div class="wrapper">
        @*<div class="sidebar" data-background-color="brown" data-active-color="danger">
            @Html.Partial("~/Views/Shared/_Sidebar.cshtml");
        </div>*@
        <div class="main-panel">
            <nav class="navbar navbar-default">
                <div class="container-fluid">
                    @Html.Partial("~/Views/Shared/_Header.cshtml");
                </div>
            </nav>
            <div class="content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>
            @Html.Partial("~/Views/Shared/_Footer.cshtml");
        </div>
    </div>
</body>
</html>

<!--   Core JS Files. Extra: TouchPunch for touch library inside jquery-ui.min.js   -->
<script src="~/Content/Layout/assets/js/jquery-3.1.1.min.js"></script>
<script src="~/Content/Layout/assets/js/jquery-ui.min.js"></script>
<script src="~/Content/Layout/assets/js/perfect-scrollbar.min.js" type="text/javascript"></script>
<script src="~/Content/Layout/assets/js/bootstrap.min.js" type="text/javascript"></script>

<!--  Forms Validations Plugin -->
<script src="~/Content/Layout/assets/js/jquery.validate.min.js"></script>

<!-- Promise Library for SweetAlert2 working on IE -->
<script src="~/Content/Layout/assets/js/es6-promise-auto.min.js"></script>

<!--  Plugin for Date Time Picker and Full Calendar Plugin-->
<script src="~/Content/Layout/assets/js/moment.min.js"></script>

<!--  Date Time Picker Plugin is included in this js file -->
<script src="~/Content/Layout/assets/js/bootstrap-datetimepicker.js"></script>

<!--  Select Picker Plugin -->
<script src="~/Content/Layout/assets/js/bootstrap-selectpicker.js"></script>

<!--  Switch and Tags Input Plugins -->
<script src="~/Content/Layout/assets/js/bootstrap-switch-tags.js"></script>

<!--  Notifications Plugin    -->
<script src="~/Content/Layout/assets/js/bootstrap-notify.js"></script>

<!-- Sweet Alert 2 plugin -->
<script src="~/Content/Layout/assets/js/sweetalert2.js"></script>

<!-- Wizard Plugin    -->
<script src="~/Content/Layout/assets/js/jquery.bootstrap.wizard.min.js"></script>

<!--  Bootstrap Table Plugin    -->
<script src="~/Content/Layout/assets/js/bootstrap-table.js"></script>

<!--  Plugin for DataTables.net  -->
<script src="~/Content/Layout/assets/js/jquery.datatables.js"></script>

<!--  Full Calendar Plugin    -->
